@model Meme
@{
    ViewData["Title"] = "Ingresar Meme";
}

<form method="post">

    

    <div class="form-group">
        <label>Titulo:</label>
        <input asp-for="Titulo"  class="form-control">
    </div>
    <div class="form-group">
        <label>Imagen del Meme</label>
        <input  asp-for="Foto" class="form-control">
    </div>
    <button class="btn btn-primary">Regisrar Meme</button>
</form>